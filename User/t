import os
os.environ.setdefault('DJANGO_SETTINGS_MODULE', '../.settings')
   
from django.contrib.auth.models import Permission

# Récupérer toutes les permissions
all_permissions = Permission.objects.all()

# Parcourir les permissions et afficher leurs informations
for permission in all_permissions:
    print("Nom de la permission :", permission.name)
    print("Code de la permission :", permission.codename)
    print("Contenu de la permission :", permission.content_type)
    print("-----------------------------------")


    # Autres champs et méthodes personnalisés
